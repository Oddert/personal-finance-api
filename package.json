{
  "name": "personal-finance-server",
  "version": "1.0.0",
  "description": "Server component for Personal Finance project",
  "scripts": {
    "test": "rm -rf dist/ && npm run build && mocha dist/test/**/*.test.js --exit",
    "test:watch": "mocha dist/test/**/*.test.js --watch",
    "test:specific:watch": "mocha -- --watch",
    "test:coverage": "npm run build && nyc --reporter=text mocha dist/test/**/*.test.js --exit",
    "start": "cross-env NODE_ENV=\"development\" concurrently \"tsc --watch\" \"nodemon dist/index.js\"",
    "start:staging": "cross-env NODE_ENV=\"staging\" nodemon dist/index.js",
    "start:production": "cross-env NODE_ENV=\"production\" nodemon dist/index.js",
    "build": "cross-env DEBUG=knex* && rm -rf dist/ && tsc",
    "build:watch": "tsc --watch",
    "db:migrate": "cd dist && knex migrate:latest",
    "db:seed": "cd dist && knex seed:run",
    "db:build": "npm run build && npm run db:migrate && npm run db:seed",
    "lint": "eslint . --config .eslintrc.json",
    "lint:fix": "eslint . --config .eslintrc.json --fix"
  },
  "author": "Robyn Felicity Heather Veitch",
  "license": "ISC",
  "dependencies": {
    "@types/knex": "^0.16.1",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "i18next": "^25.1.2",
    "i18next-express-middleware": "^2.0.0",
    "i18next-node-fs-backend": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "objection": "^3.0.1",
    "pg": "^8.14.1",
    "pg-types": "^4.0.2",
    "sqlite3": "^5.1.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/i18next-node-fs-backend": "^2.1.5",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "jsdoc": "^4.0.4",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.1.3"
  }
}
