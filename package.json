  "main": "app.js",
  "scripts": {
    "test": "rm -rf dist/ && npm run build && mocha dist/test/**/*.test.js --exit",
    "test:watch": "mocha dist/test/**/*.test.js --watch",
    "test:specific:watch": "mocha -- --watch",
    "test:coverage": "npm run build && nyc --reporter=text mocha dist/test/**/*.test.js --exit",
    "start": "concurrently \"tsc --watch\" \"nodemon dist/index.js\"",
    "build": "tsc",
    "build:watch": "tsc",
    "db:migrate": "cd dist && knex migrate:latest",
    "db:seed": "cd dist && knex seed:run",
    "db:build": "npm run build && npm run db:migrate && npm run db:seed",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
